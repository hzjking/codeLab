  <script type="text/javascript" language="javascript">


    function queryOrderListForPage(pageNo){
      $("#pageNo").val(pageNo);
      submit();
    }
    function nextPage(pageNo){
      $("#pageNo").val(pageNo+1);
      submit();
    }
    function prevPage(pageNo){
      $("#pageNo").val(pageNo-1);
      submit();
    }
    function submit(){
      document.queryRole.action = '<%=request.getContextPath()%>/user/user_role_view';
      document.queryRole.submit();
    }

    function roleFocus(){
      document.getElementById("role").style.borderColor="#D3EAF3 ";
      document.getElementById("roleDiv").innerHTML="";
    }

    function roleBlur(){
      document.getElementById("role").style.borderColor="d0cec2 ";
      var role=document.getElementById("role");
      var roleDiv=document.getElementById("roleDiv");
      var roleReg=/^[a-zA-Z]\w{3,50}$/;
      roleDiv.className="col-sm-5";
      roleDiv.innerHTML="";
      document.getElementById("roleDiv").style.color="#b20910";
      if(role.value==""){
        roleDiv.innerHTML="请输入角色！";
      }
      else if(!roleReg.test(role.value) && role.value!=""){
        roleDiv.innerHTML="角色只能以字母开头(由字母、数字、下划线组合)";
        return false;
      }else if(roleReg.test(role.value) && role.value!=""){
        document.getElementById("roleDiv").style.color="#24b92b";
        document.getElementById("roleDiv").className='col-sm-5 fa fa-check';
        return true;

      }
    }

    function roleNameFocus(){
      document.getElementById("roleName").style.borderColor="#D3EAF3 ";
      document.getElementById("roleNameDiv").innerHTML="";
    }

    function roleNameBlur(){
      document.getElementById("roleName").style.borderColor="d0cec2 ";
      var roleName=document.getElementById("roleName");
      var roleNameDiv=document.getElementById("roleNameDiv");
      var roleNameReg=new RegExp("[`~!@#$^&*()=|{}':;',\\[\\].<>/?~！@#￥……&*（）——|{}【】‘；：”“'。，、？]");
      roleNameDiv.className="col-sm-5";
      roleNameDiv.innerHTML="";
      document.getElementById("roleNameDiv").style.color="#b20910";
      if(roleName.value==""){
        roleNameDiv.innerHTML="请输入角色名称！";
      }
      else if(!roleNameReg.test(roleName.value) && roleName.value!=""){
        roleNameDiv.innerHTML="角色名称不能含有特殊字符，请检查输入！";
        return false;
      }else if(roleNameReg.test(roleName.value) && roleName.value!=""){
        document.getElementById("roleNameDiv").style.color="#24b92b";
        document.getElementById("roleNameDiv").className='col-sm-5 fa fa-check';
        return true;

      }
    }


  </script>
